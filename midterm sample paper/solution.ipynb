   "source": [
    "# sum of the numbers divisible by 2 and 3 upto n\n",
    "\n",
    "n = int(input(\"Enter the value of n: \"))\n",
    "sum = 0\n",
    "for i in range(1, n+1):\n",
    "    if i % 2 == 0 and i % 3 == 0:\n",
    "        sum += i\n",
    "print(\"Sum of the numbers divisible by 2 and 3 upto n is\", sum)"
   ]
  }
