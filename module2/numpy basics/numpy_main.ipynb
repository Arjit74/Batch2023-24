    "print(arr[1:5])\n",
    "print(arr[1:5:1])\n",
    "\n",
    "print(arr[1:5:2])\n",
    "\n",
    "print(arr[:5])\n",
    "print(arr[1:])\n",
    "print(arr[::2])\n",
