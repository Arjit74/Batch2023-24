    "# Gully Boy Boht Hard Question\n",
    "st = input()\n",
    "node = int(input())\n",
    "rp = int(input())\n",
    "\n",
    "re = st[node:] + st[:node]  # complete string with circular shift\n",
    "re = re.replace(' ', '')  # remove spaces\n",
    "re = re * rp  # repeat string\n",
    "\n",
    "print(*re)"
   ]
  }
